{
    "builds": [
      {
        "src": "api/**/*.py",
        "use": "@vercel/python"   
      },      
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/testing",
        "dest": "api/testing.py"
      },
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ]
  }
